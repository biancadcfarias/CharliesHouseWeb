@model HomeModel
@{
    ViewData["Title"] = "Confirmação de apagar o contato";
}

<div class="text-center">
    <h3>Realmente deseja apagar o cliente?</h3>
</div>

<a class="btn btn-primary btn-block" role="button" asp-controller="Client" asp-action="Index">Cancelar exclusão.</a>
<a class="btn btn-danger btn-block" role="button" asp-controller="Client" asp-action="DeleteClient">Confirmar exclusão.</a>
